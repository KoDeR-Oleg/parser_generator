{
  "manifest_version": 2,

  "name": "Parser Generator",
  "description": "This extension will parse any page",
  "version": "1.0",

  "icons": {
    "128" : "128.png"
  },

  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html",
    "default_title": "Parser Generator"
  },

  "permissions": [
    "activeTab",
    "storage",
    "pageCapture",
    "tabs"
  ]
}